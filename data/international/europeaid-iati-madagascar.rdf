        <dct:title xml:lang="ga-t-en-t0-mtec">An Coimisiún Eorpach — AS INTPA — cúnamh forbartha do Mhadagascar</dct:title>
        <dct:accrualPeriodicity>
          <dct:Frequency rdf:about="http://publications.europa.eu/resource/authority/frequency/QUARTERLY"/>
